{
  "name": "winston86/completeness",
  "description": "winston86/completeness. Open source PIM application.",
  "homepage": "https://kennerpim.de",
  "license": "GPL-3.0-only",
  "require": {
    "mekras/php-speller": "^2.1.4"
  },
  "autoload": {
    "psr-4": {
      "Completeness\\": "app/"
    }
  },
  "config": {
    "vendor-dir": "../../"
  },
  "extra": {
    "treoId": "Completeness",
    "version": "1.0.x",
    "name": {
      "default": "Completeness"
    },
    "description": {
      "default": "Completeness module for KennerPim."
    }
  },
  "scripts": {
    "post-install-cmd": ["cp -R ./client ../../../", "cp -R ../completeness ../../kennersoft/"],
    "post-update-cmd": ["cp -R ./client ../../../", "cp -R ../completeness ../../kennersoft"]
  }
}
